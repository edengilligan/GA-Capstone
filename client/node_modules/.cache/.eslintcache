esheet] = timesheets;\n    setTimesheets(newTimesheets);\n    const url = `http://localhost:3000/api/timesheets/:${deletedTimesheet.id}`;\n    console.log(url);\n\n    fetch(url, {\n      method: \"DELETE\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(deletedTimesheet),\n    }).then((response) => {\n      console.log(\"DELETE response:\", response);\n    });\n  };\n\n  return (\n    \n    <div> <h1>Timesheets</h1> \n    <ul>\n    {timesheets.map((el, index) => (\n    <button key={index} onClick={() => { handleTimesheetClick(el.id);}}>{el.client}</button>))}\n    </ul>\n              \n      <TimeForm submit={handleTimeFormSubmit} timesheet={timesheets} />\n              \n      <TimeFormEdit\n        submit={handleEditTimesheet}\n        onDelete={handleDelete}\n        timesheet={timesheetEdit}\n      />\n      {/* <TimeFormDelete\n        submit={handleDeleteTimesheet}\n        timesheet={timesheetDelete}\n      />\n            */}\n    </div>\n  );\n};\nexport default TimeContainer;\n","/Users/edengilligan/General_Assembly/GA-Project-4-Ruby/client/src/components/TimeForm.js",[],"/Users/edengilligan/General_Assembly/GA-Project-4-Ruby/client/src/components/List.js",[],"/Users/edengilligan/General_Assembly/GA-Project-4-Ruby/client/src/components/TimeFormEdit.js",[],"/Users/edengilligan/General_Assembly/GA-Capstone/client/src/index.js",[],["82","83"],"/Users/edengilligan/General_Assembly/GA-Capstone/client/src/App.js",["84","85","86","87","88"],"/Users/edengilligan/General_Assembly/GA-Capstone/client/src/reportWebVitals.js",[],"/Users/edengilligan/General_Assembly/GA-Capstone/client/src/components/TimeForm.js",[],"/Users/edengilligan/General_Assembly/GA-Capstone/client/src/components/TimeFormEdit.js",["89","90","91","92"],"/Users/edengilligan/General_Assembly/GA-Capstone/client/src/components/List.js",["93","94"],"import React, { useEffect, useState } from \"react\";\nimport { Link } from \"react-router-dom